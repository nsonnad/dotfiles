:noremap j 5j
:noremap k 5k
:noremap <S-j> <C-p>
:noremap <S-k> <C-n>
:set gui=none
:set gui=tabs
:colorscheme ocean
:hi Hint font-family: "Inconsolata", monospace; font-size: 12px; font-weight: bold; text-transform: uppercase; color: white; background-color: red; border-color: ButtonShadow; border-width: 0px; border-style: solid; padding: 0px 1px 0px 1px; 
:hi CmdLine>* font-family: "Inconsolata", monospace; font-size: 16px;
:hi CompGroup font-family: "Inconsolata", monospace; font-size: 16px;
:hi Normal color: black; background: white; font-size: 14px; font-family: Consolas, monaco, monospace;
:set titlestring=ffox
